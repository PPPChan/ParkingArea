.body {
  background: #f7f7f7;
  height: 100%;
  text-align: center;
}

.mod-title {
  width: 100%;
  margin: 0 auto;
  height: 120rpx;
  line-height: 120rpx;
  border-bottom: 1px solid #ddd;
  background: #fff;
}

.ico_log {
  display: inline-block;
  text-align: center;
  width: 240rpx;
  height: 70rpx;
  vertical-align: middle;
}

.ico-1 {
  background: url('data:image/jpeg;base64,') no-repeat;
}

.mod-ct {
  min-width: 300px;
  max-width: 640px;
  margin: 0 auto;
  margin-top: 15px;
  background: #fff url('data:image/png;base64,') top center repeat-x;
  text-align: center;
  color: #333;
  border: 1px solid #e5e5e5;
  border-top: none;
  border-bottom: none;
}

.mod-ct .order {
  font-size: 20px;
  padding-top: 10px;
}

.mod-ct .amount {
  font-size: 48px;
  margin-top: 10px;
  text-align: center;
}

image {
  width: 210px;
  aspect-ratio: auto 210 / 210;
  height: 210px;
}

.time-item strong {
  background: #3ec742;
  color: #fff;
  line-height: 25px;
  font-size: 15px;
  font-family: Arial;
  padding: 0 10px;
  margin-right: 10px;
  border-radius: 5px;
  box-shadow: 1px 1px 3px rgb(0 0 0 / 20%);
}
.mod-ct .tip {
  margin-top: 20px;
  border-top: 1px dashed #e5e5e5;
  padding: 10px 0;
  position: relative;
}

.mod-ct .tip .ico-scan {
  display: inline-block;
  width: 56px;
  height: 55px;
  background: url('data:image/png;base64,') 0 0 no-repeat;
  vertical-align: middle;
}

.mod-ct .tip {
  margin-top: 20px;
  border-top: 1px dashed #e5e5e5;
  padding: 10px 0;
  position: relative;
}
.tip-text {
  display: inline-block;
  vertical-align: middle;
  text-align: left;
  margin-left: 23px;
  font-size: 16px;
  line-height: 28px;
}
.mod-ct .detail-open {
  border-top: 1px solid #e5e5e5;
}
.mod-ct .detail {
  margin-top: 40rpx;
  padding-top: 0px;
  padding-bottom: 40rpx;
}
.mod-ct .detail .detail-ct {
  display: block;
  font-size: 40rpx;
  text-align: right;
  line-height: 80rpx;
  clear:both;
  padding: 0 40rpx;
}